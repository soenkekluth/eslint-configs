{
  "name": "eslint-config-sk",
  "version": "1.1.0",
  "description": "production ready eslint config using es6 prettier jest codemod browserlist flow ...",
  "keywords": [
    "javascript",
    "ecmascript",
    "es6",
    "prettify",
    "eslint",
    "lint",
    "lint-config",
    "config",
    "jest",
    "no-config",
    "prettier"
  ],
  "main": "index.js",
  "repository": "git@github.com:soenkekluth/eslint-configs.git",
