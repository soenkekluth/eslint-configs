    "publish:local": "yalc publish",
    "link:local": "yalc link eslint-config-sk-common"
  },
  "devDependencies": {
    "jest": "22.4.3"
  },
  "jest": {
    "preset": "jest-preset-sk",
    "rootDir": "./"
  }
