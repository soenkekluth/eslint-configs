{
  "name": "eslint-config-sk-browser",
  "version": "0.5.8",
  "description": "production ready eslint config using es6 prettier jest codemod browserlist flow ...",
  "keywords": [
    "javascript",
    "ecmascript",
    "es6",
    "prettify",
    "eslint",
    "lint",
    "lint-config",
    "config",
    "jest",
    "no-config",
    "prettier"
  ],
  "main": "index.js",
  "repository": "git@github.com:soenkekluth/eslint-configs.git",
  "author": "SÃ¶nke Kluth <mail@soenkekluth.com>",
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=4.18.2",
    "eslint-config-sk-common": ">=0.5.7"
  },
  "dependencies": {
    "eslint": "^4.18.2",
    "eslint-config-sk-common": "^0.5.8"
  },
  "engines": {
    "node": ">=9.x"
  },
  "devEngines": {
    "node": ">=9.x"
  },
  "jest": {
    "roots": [
      "./"
    ],
    "testRegex": "test/.*.js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      ".history"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/.history/**",
      "!**/vendor/**"
    ]
  },
  "scripts": {
    "publish:local": "yalc publish",
    "test": "jest --coverage --verbose",
    "lint": "yarn eslint .",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "jest": "^23.0.0-alpha.0"
  }
}
